{
    "name": "projectsaturn/appypass",
    "description": "A Laravel package to create Apple Wallet compatible passes.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": ["php", "laravel", "Apple", "Passbook"],
    "authors": [
        {
            "name": "Project Saturn Studios",
            "email": "usftechacg@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-zip": "*",
        "illuminate/support": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "phpspec/phpspec": "~2.1",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "ProjectSaturn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProjectSaturn\\Test\\": "tests"
        }
    }
}


